{
  "name": "typespeed",
  "displayName": "Typing Speed",
  "description": "Calculates and displays your typing speed (WPM) in the status bar when you stop typing",
  "version": "0.0.5",
  "license": "MIT",
  "publisher": "sincerelyyyash",
  "repository": {
    "type": "git",
    "url": "https://github.com/sincerelyyyash/typespeed"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "typing",
    "speed",
    "wpm",
    "words per minute",
    "typing speed",
    "productivity"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typespeed.showMenu",
        "title": "Show Menu",
        "icon": "$(zap)"
      },
      {
        "command": "typespeed.resetHighscore",
        "title": "Reset Highscore WPM"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
